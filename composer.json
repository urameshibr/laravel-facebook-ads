{
  "name": "agencia-bid/laravel-facebook-ads",
  "description": "Facebook & Instagram Ads for Laravel 5.6+",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "guilherme.zanini",
      "email": "guilherme@agencia.bid"
    }
  ],
  "require": {
    "php": ">=7.1.13",
    "facebook/php-business-sdk": "^7.0.4 || ^10.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13",
    "orchestra/testbench": "~3.0",
    "phpunit/phpunit": "~7.3 || ~8.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Agenciabid\\LaravelFacebookAds\\Providers\\LaravelFacebookServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Agenciabid\\LaravelFacebookAds\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelFacebookAds\\Test\\": "tests/"
    }
  }
}
